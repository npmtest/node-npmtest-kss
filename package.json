{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "description": "#### basic test coverage for  [kss (v2.4.0)](http://kss-node.github.io/kss-node)  [![npm package](https://img.shields.io/npm/v/npmtest-kss.svg?style=flat-square)](https://www.npmjs.org/package/npmtest-kss) [![travis-ci.org build-status](https://api.travis-ci.org/npmtest/node-npmtest-kss.svg)](https://travis-ci.org/npmtest/node-npmtest-kss)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "kss": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmtest/node-npmtest-kss",
    "keywords": [
        "coverage",
        "test",
        "kss"
    ],
    "license": "MIT",
    "main": "lib.npmtest_kss.js",
    "name": "npmtest-kss",
    "nameAlias": "npmtest_kss",
    "nameOriginal": "npmtest-kss",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmtest/node-npmtest-kss.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "2017.4.26"
}
